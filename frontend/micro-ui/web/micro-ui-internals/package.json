    "dev:death": "cd packages/modules/death && yarn start",
    "build:death": "cd packages/modules/death && yarn build"
  },
  "devDependencies": {
    "husky": "7.0.4",
    "lint-staged": "12.3.7",
    "npm-run-all": "4.1.5",
    "prettier": "2.1.2",
    "@babel/plugin-transform-optional-chaining": "7.25.9"
  },
  "husky": {},
  "lint-staged": {
    "*.{js,css,md}": "prettier --write"
  },
  "dependencies": {
    "lodash": "4.17.21",
    "microbundle-crl": "0.13.11"
  } ,
