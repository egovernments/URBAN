  "lint-staged": {
    "*.{js,css,md}": "prettier --write"
  },
  "dependencies": {
    "lodash": "4.17.21",
    "microbundle-crl": "0.13.11"
  },
  "resolutions": {
    "**/babel-loader": "8.2.2",
    "**/@babel/core": "7.14.0",
    "**/@babel/preset-env": "7.14.0",
